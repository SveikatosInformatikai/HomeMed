    <CheckBox
        android:id="@+id/check_odos_gleiviniu_pageltimai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginEnd="61dp"
        android:text="@string/simp_odos_gleiviniu_pageltimai"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/check_odos_gleiviniu_pabrinkimai"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_ugio_kuno_proporcciju_pokyciai" />

    <CheckBox
        android:id="@+id/check_odos_gleiviniu_pabrinkimai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginEnd="50dp"
        android:text="@string/simp_odos_gleivinių_pabrinkimai"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/check_suliesejimas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_odos_gleiviniu_pageltimai" />

    <CheckBox
        android:id="@+id/check_suliesejimas"
