        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_mirgejimas_akyse" />

    <Button
        android:id="@+id/button_ivedimas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="123dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="123dp"
        android:layout_marginBottom="16dp"
        android:text="@string/text_sutrikimu_ivedimas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_kuno_mases_padidejimas"
        app:layout_constraintVertical_bias="0.87" />

    <CheckBox
        android:id="@+id/check_kuno_mases_padidejimas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginEnd="80dp"
        android:text="@string/simp_kuno_mases_padidejimas"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/check_ugio_kuno_proporcciju_pokyciai"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_svokstimas_krutineje" />

</androidx.constraintlayout.widget.ConstraintLayout>