        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/check_mirgejimas_akyse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginEnd="149dp"
        android:text="@string/simp_mirgejimas_akyse"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_regos_sutrikimas" />

    <CheckBox
        android:id="@+id/check_rijimo_sutr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginEnd="149dp"
        android:text="@string/simp_rijimo_sutrikimas"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
