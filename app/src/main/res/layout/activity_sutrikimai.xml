        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_drebulys" />

    <CheckBox
        android:id="@+id/check_drebulys"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginEnd="229dp"
        android:text="@string/simp_drebulys"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_uzimas_ausyse" />

    <CheckBox
        android:id="@+id/check_svokstimas_krutineje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
